<!--miniprogram/pages/home/home.wxml-->
<scroll-view scroll-y="true" class="container">
  <view class="label-card">
    <view class="label-card__body"></view>

  </view>
  <view class="timeline">
    <view wx:for="{{diaryData}}" wx:for-item="diary" class="item">
      <view class="line"></view>
      <icon class="icon"></icon>
      <view class="content">
        <text class="time">{{diary.time}}</text>
        <view class="diary-detail">
          <block wx:if="diary.meida==='image'" wx:for="{{diary.images}}" wx:for-item="image">

            <image mode="aspectFill" src="{{image.url}}"></image>

          </block>
            <text class="desc">{{diary.desc}}</text>
        </view>

      </view>

    </view>
  </view>

</scroll-view>